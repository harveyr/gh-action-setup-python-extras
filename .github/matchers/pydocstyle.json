{
  "problemMatcher": [
    {
      "owner": "pydocstyle",
      "pattern": [
        {
          "regexp": "^(\\S+):(\\d+) (in.+):$",
          "file": 1,
          "line": 2,
          "message": 3
        },
        {
          "regexp": "^.+(D\\d+): (.+)$",
          "code": 1,
          "message": 2,
          "loop": true
        }
      ]
    }
  ]
}
